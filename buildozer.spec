package.name = englishdictation
package.domain = org.learn
version = 1.0.0
requirements = python3,kivy==2.3.0
android.api = 24
android.ndk = 25b
android.build_tools = 24.0.3
android.permissions = INTERNET,WRITE_EXTERNAL_STORAGE,READ_EXTERNAL_STORAGE
android.archs = arm64-v8a,armeabi-v7a
description = 中小学英语听写学习机，适配人教版教材，支持入学测试、自动学习计划、课本原文查阅，帮助孩子轻松掌握单词拼写！
author = 你的名字
author.email = your@email.com
author.url = 

[buildozer]
